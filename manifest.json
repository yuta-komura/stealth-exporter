{
  "manifest_version": 3,
  "name": "StealthExporter",
  "version": "1.0",
  "description": "ローカルストレージのデータを取得し、Postman Echo API に送信する",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
